		.redtext {color:red;}
		.titlecolor{color:yellow;}
		
		
		div {border:2px solid red;
			position:absolute;
			<!--There are three different types of postition; 
			"absolute", "relative", "fixed"-->
			width:300px;
			<!--top:40px;-->
			left:35;
			}
		
		<!-- "div" ability to define sections of the webpage -->
		<!-- h1, p {color: red;} 
		Style more than one elements-->
		a:link{color:red;
		text-decoration:none;
		text-align:left;}
		a:visited{color:pink;}
		a:hover{background-color:lightgreen;
				color:black;
				text-decoration:underline;
				font-weight:bold;}
		a:active{background-color:orange;}		
		<!--"link" for example called the "state". CSS code for a-->
		
		h1 {text-align:left;
			background-color:white;
			border: 1px solid red;
			}
		
		body {background-color:black;
			  text-align:center;
			  font-family:courier new;
			  color:teal;
			  background-image:url(0d4be9fa703238591105d1013006ac73.jpg);
			  background-repeat:no-repeat;
			  background-position: 50% 25%;
			  <!--background-position: 200px 200px; -->
			  <!-- background-repeat:repeat-y or x; -->
		}
		
		#001{border:5px solid blue;
			position:absolute;
			width:300px;
			left:450;}
			
		#002{border:2px solid red;
			 position:absolute;
			 width:500px;
			 left:200;}
		
		p  {line-height:150%;
			text-align:left;
			background-color:;
			color:purple;
			font-family:courier new;
			text-indent:50px;
			margin: 50px;
			<!--padding is inside the border, margin is outside the border
			margin-left/right/top:;
			-->
			}
		span{font-weight:bold;
			color:yellow;}
		
		table {border:3px solid teal;}
		
		
		tr {font-family:
			courier new;
			background-color:;
			width: px;
			height: px;
			border: 3px solid teal;
			<!-- there's probably shortcuts like the one above^^^^ -->
			<!-- not always useful but it can be necessary sometimes.-->
			}
			
		ol {font-family:courier new;
			border 3px solid lightgreen;
			text-align:left;
			}
		
		li {font-weight:bold;
			border-top-color:yellow;
			border-top-style:solid;
			border-top-width:4px;
			background-color:white;
			text-align:left;
			}
			
			
		ul {list-style-type:hiragana;
		    text-align:left;}	
		<!--{list-style-image:url(___.png)}
			{list-style-type:none;}
			{border: 1px solid blue}
			padding:0px;
		-->
		
		h4 {text-align:left;
			}
		
		h3 {text-align:left;
